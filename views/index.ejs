    <script>  function checkScreenSize() {
      const screenWarning = document.getElementById('screenWarning');
      if (window.innerWidth < 1000) {
          screenWarning.classList.remove('hidden');
          setTimeout(() => {
              screenWarning.classList.add('hidden');
          }, 10000); // Hide after 5 seconds
      }
  }

  window.addEventListener('resize', checkScreenSize);
  window.addEventListener('load', checkScreenSize);</script>
